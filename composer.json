{
    "name": "nunomaduro/ohdear-cli",
    "description": "An Oh Dear CLI tool written in PHP with Laravel Zero.",
    "keywords": ["framework", "laravel", "console", "zero", "cli", "oh dear", "ohdear"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/ohdear-cli/issues",
        "source": "https://github.com/nunomaduro/ohdear-cli"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "laravel-zero/framework": "^8.0",
        "ohdearapp/ohdear-php-sdk": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.1",
        "phpunit/phpunit": "~9.3",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["ohdear-cli"]
}
