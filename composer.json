{
    "name": "nunomaduro/ohdear-cli",
    "description": "An Oh Dear CLI tool written in PHP with Laravel Zero.",
    "keywords": ["framework", "laravel", "console", "zero", "cli", "oh dear", "ohdear"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/ohdear-cli/issues",
        "source": "https://github.com/nunomaduro/ohdear-cli"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "vlucas/phpdotenv": "^2.4",
        "ohdear/forge-sync": "^0.2.0",
        "laravel-zero/framework": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["ohdear-cli"]
}
